{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "cloudrest-svc",
    "namespace": "default"
  },
  "spec": {
    "selector": {
      "app": "cloudrest-app"
    },
    "ports": [
      {
        "port": 80,
        "targetPort": 80,
        "name": "http"
      },
      {
        "port": 443,
        "targetPort": 443,
        "name": "https"
      }
    ],
    "type": "LoadBalancer",
    "loadBalancerIP": "your.reserved.ip.address"
  }
}
